<p id="notice"><%= notice %></p>

<h1>Student Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Stud</th>
      <th>Subject</th>
      <th>Firts sem</th>
      <th>Second sem</th>
      <th>Trird sem</th>
      <th>Fourth sem</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_subjects.each do |student_subject| %>
      <tr>
        <td><%#= student_subject.stud_id %></td>
        <td><%= student_subject.subject_id %></td>
        <td><%#= student_subject.firts_sem %></td>
        <td><%#= student_subject.second_sem %></td>
        <td><%#= student_subject.trird_sem %></td>
        <td><%#= student_subject.fourth_sem %></td>
        <td><%= link_to 'Show', student_subject %></td>
        <td><%= link_to 'Edit', edit_student_subject_path(student_subject) %></td>
        <td><%= link_to 'Destroy', student_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Subject', new_student_subject_path %>
